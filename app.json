{
  "expo": {
    "name": "Reminder",
    "slug": "reminder",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1024x.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1891e9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F2F2F2"
    },
    "notification": {
      "icon": "./assets/iconNotification.png",
      "color": "#1891e9"
    },
    "android": {
      "package": "com.brunocosta.reminder",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#1891e9",
        "foregroundImage": "./assets/iconShape.png"
      }
    }
  }
}
